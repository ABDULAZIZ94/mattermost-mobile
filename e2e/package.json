{
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@babel/register": "7.7.4",
    "@babel/traverse": "7.7.4",
    "@babel/types": "7.7.4",
    "@wdio/appium-service": "5.16.11",
    "@wdio/cli": "5.16.16",
    "@wdio/local-runner": "5.16.15",
    "@wdio/mocha-framework": "5.16.15",
    "@wdio/spec-reporter": "5.16.11",
    "@wdio/sync": "5.16.15",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "node-fetch": "2.6.0",
    "npm-run-all": "4.1.5",
    "webdriverio": "5.16.15"
  },
  "scripts": {
    "build:android": "cd .. && make clean && make unsigned-android && cd ..",
    "build:ios": "cd .. && make clean && make ios-sim-x86_64 && cd ..",
    "test:android": "wdio ./config/android.conf.js",
    "test:ios": "wdio ./config/ios.conf.js",
    "test:android-dev": "DEV=true wdio ./config/android.conf.js",
    "test:ios-dev": "DEV=true wdio ./config/ios.conf.js",
    "build:test:android": "npm-run-all build:android test:android",
    "build:test:ios": "npm-run-all build:ios test:ios",
    "test": "npm-run-all build:test:android build:test:ios"
  }
}
